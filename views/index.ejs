<% include header %>
<% if (threads.length) { %>
  <% threads.forEach(function(thread) { %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <%= thread.title %>
        
      </div>
      <div class="panel-body">
        <%= thread.content %>
      <a href= '/delete/<%= thread.tid %>'class="btn btn-primary pull-right"
          onclick="return confirm('Are you sure you want to delete')">
          DELETE
      </a>
      <a href= '/edit/<%= thread.tid %>'class="btn btn-primary pull-right"
        onclick='f'>
          EDIT
      </a>
      <a href= '/view/<%= thread.tid %>'class="btn btn-primary pull-right">
        VIEW
    </a>
      </div>
    </div>
  <% }) %>
<% } else { %>
  No entries! <a href="/new-entry">Add one!</a>
<% } %>
<% include footer %>